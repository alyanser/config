! tilde without shift  
keycode 49 = asciitilde grave asciitilde grave

! right shift as backspace
remove shift = Shift_R
keycode 62 = BackSpace BackSpace BackSpace BackSpace

! swap caps_lock with ctrl_l
remove Lock = Caps_Lock
remove Control = Control_L
keysym Caps_Lock = Control_L
keysym Control_L = Caps_Lock
add Lock = Caps_Lock
add Control = Control_L

! map tab to some random key for xcape
keycode 255 = Tab ISO_Left_Tab Tab ISO_Left_Tab

! make tab Super_L
keycode 23 = Super_L Super_L Super_L Super_L 
