font_family cascadia code semibold
bold_font cascadia code 
italic_font auto
bold_italic_font auto
font_size 10

cursor_shape underline
cursor_underline_thickness 10
shell_integration disabled

repaint_delay 3
input_delay 3
# sync_to_monitor no 

enable_audio_bell no

window_margin_width 0
single_window_margin_width -1
window_padding_width 5

tab_bar_style powerline 

map ctrl+c copy_and_clear_or_interrupt
map ctrl+v paste_from_clipboard
map ctrl+j next_window
map ctrl+k previous_window
map ctrl+- launch --cwd=current
map ctrl+alt+j next_tab
map ctrl+alt+k previous_tab
map ctrl+t launch --cwd=current --type=tab
map ctrl+1 goto_tab 1
map ctrl+2 goto_tab 2
map ctrl+3 goto_tab 3
map ctrl+4 goto_tab 4
map ctrl+5 goto_tab 5
map ctrl+6 goto_tab 6
map ctrl+7 goto_tab 7
map ctrl+8 goto_tab 8
map ctrl+9 goto_tab 9
map ctrl+w close_window
